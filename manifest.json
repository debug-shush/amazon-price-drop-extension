{
    "manifest_version": 2,
  
    "name": "amazon price drop extension",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.1",
    "background": {
      "scripts": ["background.js"]
    },
  
    "browser_action": {
      
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "https://www.amazon.in/"
    ]
  }